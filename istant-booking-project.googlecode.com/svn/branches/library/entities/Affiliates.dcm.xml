<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Affiliates" table="affiliates">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id"/>
    <id name="email" type="string" column="email"/>
    <field name="name" type="string" column="name" length="255"/>
    <field name="address" type="string" column="address" length="255"/>
    <field name="city" type="string" column="city" length="255"/>
    <field name="telephoneNumber" type="string" column="telephone_number" length="20"/>
    <field name="password" type="string" column="password" length="255"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
